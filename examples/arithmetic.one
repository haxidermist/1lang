// arithmetic.one - Basic arithmetic operations

function main:
  outputs:
    exit_code: Integer
  implementation:
    a = 10
    b = 5

    sum = a + b
    diff = a - b
    prod = a * b
    quot = a / b

    println("Arithmetic operations:")
    println(sum)
    println(diff)
    println(prod)
    println(quot)

    return 0
